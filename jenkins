pipeline {
    agent {
         node {
            label 'mvn'
            }
    }
    stages {
        
        stage('vcs') {

            steps {
                git url: 'https://github.com/Praveen-711/openmrs-core.git', 
                    branch: 'master'
            }
        }
    
        stage ('Exec Maven') {
            steps {
                rtMavenRun (
                    tool: 'MAVEN_TOOL', 
                    pom: 'pom.xml',
                    goals: 'package',
                   
                )
            }
    } 
      stage ('Artifactory config'){
             steps{

                   rtMavenDeployer (
                    id: "MAVEN_DEPLOYER",
                    serverId: "JFROG_new",
                    releaseRepo: "praveen-libs-release-local",
                    snapshotRepo: "praveen-libs-snapshot-local"
                )

        }
    }
               
    }
}