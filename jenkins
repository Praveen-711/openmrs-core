pipeline {
    agent {
         node {
            label 'mvn'
            }
    }
    stages {
        
        stage('vcs') {

            steps {
                git url: 'https://github.com/Praveen-711/openmrs-core.git', 
                    branch: 'master'
            }
        }
        
         stage('build'){

            steps{
                sh 'mvn package'
            }
        }
    }
    }